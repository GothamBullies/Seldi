<section class="nearby-place" id="associates">

    <div class="container">

        <div class="row">

            <div class="col-md-12">

                <div class="nearby">

                    <h1 class="section-heading wow fadeInUp" data-wow-duration="1.2s" data-wow-delay="0s" style="visibility: visible; animation-duration: 1.2s; animation-delay: 0s; animation-name: fadeInUp;">EXECUTIVE <span>TEAM</span></h1>                   

